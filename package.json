{
  "name": "@strapi-community/strapi-plugin-cloudinary-media-library",
  "version": "1.0.0-beta.1",
  "type": "commonjs",
  "description": "Cloudinary Media Library Custom Field for Strapi",
  "keywords": [
    "strapi",
    "cloudinary",
    "media",
    "library",
    "custom field",
    "plugin"
  ],
  "strapi": {
    "kind": "plugin",
    "name": "cloudinary-media-library",
    "displayName": "Strapi cloudinary media library",
    "description": "Custom Cloudinary media library integration for Strapi, with enhanced upload and management features."
  },
  "author": "Pierre Burgy <pierre@strapi.io>",
  "maintainers": [
    {
      "name": "Mateusz Kleszcz",
      "email": "mkleszcz@virtuslab.com"
    }
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn test:unit"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build": "yarn clean && strapi-plugin build --minify",
    "clean": "rm -rf dist",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:unit": "jest --forceExit --detectOpenHandles --config server/jest.config.js",
    "test:unit:watch": "jest --forceExit --detectOpenHandles --config server/jest.config.js --watch",
    "test:unit:ci": "CI=true jest --ci --runInBand --verbose --coverage --config server/jest.config.js"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.23",
    "@strapi/icons": "^2.0.0-rc.23",
    "@tanstack/react-query": "^5.74.11",
    "react-helmet": "^6.1.0",
    "react-intl": "^7.1.6",
    "react-script-hook": "^1.7.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.0",
    "@strapi/typescript-utils": "^5.12.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "6.1.8",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.0.0",
    "@strapi/strapi": "^5.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
